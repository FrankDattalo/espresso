(def fibonacciiter (fn (n prev curr iter)
    (if (greaterThanOrEqual iter n)
        curr
        (fibonacciiter n curr (add prev curr) (add iter 1)))))

(def fibonacci (fn (n)
    (fibonacciiter n 1 1 1)))

(println (fibonacci  1))
(println (fibonacci  2))
